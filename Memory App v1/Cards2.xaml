    <Page.BottomAppBar>
        <CommandBar Background="#FF0A320A" BorderBrush="Transparent" >
            <CommandBar.PrimaryCommands>
                <AppBarButton x:Name="appbarbuttonCaption" Label="Show Answer's Transcript" Icon="Caption"  Click="appbarbuttonCaption_Click" />
                <AppBarButton x:Name="appbarbuttonRedoOptions" Label="Repeat Options" Icon="More" >
                    <AppBarButton.Flyout>
                        <MenuFlyout>
                            <ToggleMenuFlyoutItem x:Name="commandbar_options_toggle1" Click="commandbar_options_toggle1_Click">Automatic Repeat in 1s</ToggleMenuFlyoutItem>
                            <ToggleMenuFlyoutItem x:Name="commandbar_options_toggle2" Click="commandbar_options_toggle2_Click">Automatic Repeat in 5s</ToggleMenuFlyoutItem>
                            <ToggleMenuFlyoutItem x:Name="commandbar_options_toggle3" Click="commandbar_options_toggle3_Click">Automatic Repeat in 10s</ToggleMenuFlyoutItem>
                            <ToggleMenuFlyoutItem x:Name="commandbar_options_toggle4" Click="commandbar_options_toggle4_Click">Automatic Repeat in 20s</ToggleMenuFlyoutItem>
                        </MenuFlyout>
                    </AppBarButton.Flyout>
                </AppBarButton>
                <AppBarButton Icon="Redo" Label="New Cards" Click="AppBarButton_Click_2"/>
            </CommandBar.PrimaryCommands>
            <CommandBar.SecondaryCommands>
                <AppBarButton Icon="Back" Label="Practice Page" Click="AppBarButton_Click"/>
                <AppBarButton Icon="Home" Label="Main Menu" Click="AppBarButton_Click_1" />
            </CommandBar.SecondaryCommands>
        </CommandBar>
    </Page.BottomAppBar>




